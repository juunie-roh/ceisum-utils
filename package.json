    "@typescript-eslint/parser": "^8.28.0",
    "cesium": "^1.127.0",
    "eslint": "^9.23.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
